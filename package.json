{
  "private": true,
  "name": "elvenbrill.xyz",
  "version": "1.0.0",
  "description": "The personal website of @elvenbrill",
  "author": "@elvenbrill",
  "homepage": "https://github.com/elvenbrill/elvenbrill.xyz#readme",
  "repository": "github:elvenbrill/elvenbrill.xyz",
  "bugs": "https://github.com/elvenbrill/elvenbrill.xyz/issues",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --incremental --serve",
    "update": "yarn upgrade-interactive --latest && yarn upgrade",
    "format": "prettier --write **/*.{js,json,s?css} !_site"

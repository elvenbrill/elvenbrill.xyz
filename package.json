  "devDependencies": {
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false
  }
